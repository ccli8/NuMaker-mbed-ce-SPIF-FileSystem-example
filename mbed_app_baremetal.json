{
     "requires": [
        "bare-metal",
        "rtos-api",
        "filesystem",
        "littlefs",
        "fat_chan",
        "spif-driver",
        "mbed-trace"
    ],
    "config": {
    },
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate"          : 115200,
            "platform.stdio-convert-newlines"   : true
        },
        "NUMAKER_PFM_NANO130": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "D11",
            "spif-driver.SPI_MISO"              : "D12",
            "spif-driver.SPI_CLK"               : "D13",
            "spif-driver.SPI_CS"                : "D10"
        },
        "NUMAKER_PFM_NUC472": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PF_0",
            "spif-driver.SPI_MISO"              : "PD_15",
            "spif-driver.SPI_CLK"               : "PD_14",
            "spif-driver.SPI_CS"                : "PD_13"
        },
        "NUMAKER_PFM_M453": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PD_13",
            "spif-driver.SPI_MISO"              : "PD_14",
            "spif-driver.SPI_CLK"               : "PD_15",
            "spif-driver.SPI_CS"                : "PD_12"
        },
        "NUMAKER_IOT_M467": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PJ_1",
            "spif-driver.SPI_MISO"              : "PJ_0",
            "spif-driver.SPI_CLK"               : "PI_15",
            "spif-driver.SPI_CS"                : "PI_14"
        },
        "NUMAKER_PFM_M487": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PC_0",
            "spif-driver.SPI_MISO"              : "PC_1",
            "spif-driver.SPI_CLK"               : "PC_2",
            "spif-driver.SPI_CS"                : "PC_3"
        },
        "NUMAKER_IOT_M487": {
            "target.components_add"             : ["SPIF"],
            "spif-driver.SPI_MOSI"              : "PC_0",
            "spif-driver.SPI_MISO"              : "PC_1",
            "spif-driver.SPI_CLK"               : "PC_2",
            "spif-driver.SPI_CS"                : "PC_3"
        }
    }
}